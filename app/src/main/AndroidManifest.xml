<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jmzd.ghazal.javamvvm">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.JavaMvvm"
        android:usesCleartextTraffic="true">
        <activity android:name=".view.RoomDbActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.WorkerActivity_onTime"></activity>
        <activity android:name=".view.WorkerActivity">

        </activity>
        <activity android:name=".view.SliderActivity"></activity>
        <activity android:name=".view.RegisterActivity" />
        <activity android:name=".view.LoginActivity" />
        <activity android:name=".view.RecyclerViewActivity" />
        <activity android:name=".view.NavigationActivity" />
        <activity android:name=".view.MainActivity" />
    </application>

</manifest>